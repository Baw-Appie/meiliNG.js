{
  "version": "0.0.1",
  "allowLogin": [
    "http://localhost:3000",
    "https://meiling-dev.stella-it.com",
    "https://appielogin.netlify.app"
  ],
  "invalidate": {
    "oauth": {
      "AUTHORIZATION_CODE": 300,
      "ACCESS_TOKEN": 7200,
      "REFRESH_TOKEN": 604800,
      "ACCOUNT_TOKEN": -1
    },
    "meiling": {
      "CHALLENGE_TOKEN": 300
    }
  },
  "session": {
    "v1": {
      "dataPath": "./tokenSession.json",
      "maxAge": 604800,
      "rateLimit": {
        "maxTokenPerIP": 20,
        "timeframe": 600
      },
      "debugTokens": [
        "HongMeiling"
      ]
    }
  },
  "errorFormatURL": "https://opensource.stella-it.com/developers/docs/meiling/error/{{errorCode}}.html",
  "token": {
    "default": {
      "chars": "QWERTYUIOPASDFGHJKLZXCVBNMqwertyuiopasdfghjklzxcvbnm1234567890",
      "length": 128
    }
  },
  "behindProxy": true,
  "listeningPort": 8080,
  "oauth2": {
    "skipAuthentication": [
      "38e94511-3ec2-40fd-89cb-9be6281e9862"
    ]
  }
}